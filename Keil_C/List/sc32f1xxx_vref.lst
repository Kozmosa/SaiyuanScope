S/**
S * @brief System analog circuit Vref selection.
S * @param  Vrefx[out]: Only VREF can be select the Vrefx peripheral.
S *                  - VREF:select VREF peripheral
S * @param  Peripheral_Voltage[in]: specifies the Peripheral_Voltage to check.
S *                  - Vref_ReferenceVoltageNone :VREF Reference Voltage  None
S *                  - Vref_ExReferenceVoltage:VREF ExReference Voltage 
S *                  - Vref_InReferenceVoltageEx_2_048:VREF InReferenceVoltageEx 2.048  
S *                  - Vref_InReferenceVoltageEx_1_024:VREF InReferenceVoltageEx 1.024 
S *                  - Vref_InReferenceVoltageEx_2_4:VREF InReferenceVoltageEx 2.4  
S *                  - Vref_InReferenceVoltage_2_048:Vref_ReferenceVoltage:2.048 
S *                  - Vref_InReferenceVoltage_1_024:Vref_ReferenceVoltage:1.024 
S *                  - Vref_InReferenceVoltage_2_4 :Vref_ReferenceVoltage:2.4
S
S * @retval None
S */
SErrorStatus Vref_ReferenceVoltageConfig(VREF_TypeDef* Vrefx, Vref_ReferenceVoltage_TypeDef Vref_ReferenceVoltage)
S{
S     uint32_t tmpreg;
S	
S    /* Check the parameters */
S    assert_param(IS_Vref_ALL_PERIPH(Vrefx));
S    assert_param(IS_Vref_ReferenceVoltage(Vref_ReferenceVoltage));
S    
S
S    tmpreg  =   Vrefx->VREF_CFG;
S    tmpreg &= ~(VREF_CFG_VREFS|0x03);
S    tmpreg |= (uint32_t)Vref_ReferenceVoltage;
S    Vrefx->VREF_CFG =  tmpreg;
S	 if((VREF->VREF_CFG & 0x03)==0x01)
S	 {
S		 if((uint8_t)(VREF->VREF_CFG & VREF_CFG_VREFS) == ((Vref_ReferenceVoltage)&0xFC))
S				return SUCCESS;
S		 else
S				return ERROR;
S   }
S	 else
S		 return SUCCESS;
S}
S
S
S
S/**
S * @brief  Enabled or Disable VMID port 
S * @param  Vrefx[out]: Only VREF can be select the Vrefx peripheral.
S *                  - VREF:select VREF peripheral
S * @param  NewState[in]: new state of the VMID port.
S *                  - DISABLE:Function disable
S *                  - ENABLE:Function enable 
S * @retval None
S */
Svoid Vref_VMIDCmd(VREF_TypeDef* Vrefx, FunctionalState NewState)
S{
S    assert_param(IS_Vref_ALL_PERIPH(Vrefx));
S    assert_param(IS_FUNCTIONAL_STATE(NewState));
S    if(NewState != DISABLE)
S    {
S        Vrefx->VREF_CFG |= VREF_CFG_VMIDEN;
S    }
S    else
S    {
S        Vrefx->VREF_CFG &= (~(VREF_CFG_VMIDEN));
S    }
S}
S/**
S * @brief  The internal reference divider circuit is enabled
S * @param  Vrefx[out]: Only VREF can be select the Vrefx peripheral.
S *                  - VREF:select VREF peripheral
S * @param  NewState[in]: new state of the DIVEN .
S *                  - DISABLE:Function disable
S *                  - ENABLE:Function enable 
S * @retval None
S */
Svoid Vref_DIVENCmd(VREF_TypeDef* Vrefx, FunctionalState NewState)
S{
S	  assert_param(IS_Vref_ALL_PERIPH(Vrefx));
S    assert_param(IS_FUNCTIONAL_STATE(NewState));
S    if(NewState != DISABLE)
S    {
S        Vrefx->VREF_CFG |= VREF_CFG_DIVEN;
S    }
S    else
S    {
S        Vrefx->VREF_CFG &= (~(VREF_CFG_DIVEN));
S    }
S}

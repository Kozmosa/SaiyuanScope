S/**
S * @brief  DeInitializes the TS peripheral
S * @retval None
S */
Svoid TemperSensor_DeInit(void)
S{
S  /* Enable TS reset state */
S   TS->TS_CFG = 0x00;
S}
S
S/**
S * @brief Enabled or Disable TS peripheral  
S * @param  TSx[out]:where can to select the TS peripheral.
S *                  - TS:Timer peripheral select TS
S * @param  NewState[in]: new state of the TS .
S *         - ENABLE: Function enable   
S *         - DISABLE:Function disable
S * @retval None
S */
Svoid TemperSensor_Cmd(TS_TypeDef* TSx, FunctionalState NewState)
S{
S  /* Enable TS reset state */
S	 if(NewState != DISABLE)
S  {
S    /* Enable the TS  */
S    TSx->TS_CFG |= TemperSensor_Enable;
S  }
S  else
S  {
S    /* Disable the TS  */
S     TSx->TS_CFG &= (uint8_t)~TemperSensor_Enable;
S  }
S   
S}
S
S/**
S * @brief  The TS peripheral triggers the temperature reading
S * @param  TSx[out]:where can to select the TS peripheral.
S *                  - TS:Timer peripheral select TS
S * @param  NewState[in]: new state of the TS Offset .
S *         - ENABLE: Function enable   
S *         - DISABLE:Function disable
S * @retval None
S */
Svoid TemperSensor_OffSetCmd(TS_TypeDef* TSx)
S{
S  
S 
S    TSx->TS_CFG |= 0x01<<TS_CFG_CHOP_Pos;	
S 
S}
S/**
S * @brief  The TS peripheral triggers the temperature reading
S * @param  TSx[out]:where can to select the TS peripheral.
S *                  - TS:Timer peripheral select TS
S * @param  NewState[in]: new state of the TS Offset .
S *         - ENABLE: Function enable   
S *         - DISABLE:Function disable
S * @retval None
S */
Svoid TemperSensor_OffResetCmd(TS_TypeDef* TSx)
S{ 
S
S    TSx->TS_CFG &=~ (0x01<<TS_CFG_CHOP_Pos);
S
S 
S}
S/**
S * @brief  Return the TS check data.
S * @param  None
S * @retval the TS check data.
S */
Suint16_t TemperSensor_ReadValue(void)
S{
S	uint16_t TS_Value;
S	TS_Value =((*(uint8_t*)0x8c00480)<<8)+(*(uint8_t*)0x8c00481);
S	return TS_Value;
S}
